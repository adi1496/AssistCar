{
  "name": "assistcar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:scss": "node-sass scss/main.scss css/style.css -w",
    "compile:scss": "node-sass scss/main.scss css/style.compiled.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.compiled.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:scss prefix:css compress:css",
    "watch:js": "parcel watch ./js/index.js --out-dir ./js --out-file bundle.js",
    "build:js": "parcel build ./js/index.js --out-dir ./js --out-file bundle.js"
  },
  "author": "Adrian Matei",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "axios": "^0.20.0"
  }
}
