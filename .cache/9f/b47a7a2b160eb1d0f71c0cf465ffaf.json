in', popup);\n        if(time !== 0){\n            setTimeout(() => {\n                document.querySelector('.alert-popup').remove();\n            }, time * 1000);\n        }\n    }else if(status === 'success') {\n        const popup = renderAlertPopup(message, '');\n        document.querySelector(parent).insertAdjacentElement('afterbegin', popup);\n        if(time !== 0){\n            setTimeout(() => {\n                if(reload === 'reload'){\n                    location.reload();\n                }else if(reload === '') {\n\n                }else{\n                    window.location.href = reload;\n                }\n                document.querySelector('.alert-popup').remove();\n            }, time * 1000);\n        }\n    }\n}\n\nconst hideAlertMessages = (sec) => {\n    setTimeout(() => {\n        document.querySelector('.alert-popup').remove();\n    }, sec * 1000);\n}"},"lineCount":null}},"error":null,"hash":"8d7223a32dd60ebf6b0181fe9babd0c3","cacheData":{"env":{}}}