{
  "name": "assistcar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./backend/server.js",
    "start": "node ./backend/server.js",
    "debug": "ndb ./backend/server.js",
    "watch:scss": "node-sass ./frontend/scss/main.scss ./frontend/css/style.css -w",
    "compile:scss": "node-sass ./frontend/scss/main.scss ./frontend/css/style.compiled.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./frontend/css/style.compiled.css -o ./frontend/css/style.prefix.css",
    "compress:css": "node-sass ./frontend/css/style.prefix.css ./frontend/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:scss prefix:css compress:css",
    "watch:js": "parcel watch ./frontend/js/index.js --out-dir ./frontend/js --out-file bundle.js",
    "build:js": "parcel build ./frontend/js/index.js --out-dir ./frontend/js --out-file bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adi1496/AssistCar.git"
  },
  "author": "Adrian Matei",
  "license": "ISC",
  "devDependencies": {
    "ndb": "^1.1.5",
    "nodemon": "^2.0.4",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "@babel/polyfill": "^7.11.5",
    "axios": "^0.20.0"
  },
  "bugs": {
    "url": "https://github.com/adi1496/AssistCar/issues"
  },
  "homepage": "https://github.com/adi1496/AssistCar#readme"
}
